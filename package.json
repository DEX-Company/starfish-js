{
  "name": "@dex-company/starfish-js",
  "version": "0.0.2",
  "description": "JavaScript Starfish client library",
  "main": "./dist/node/starfish.js",
  "browser": "./dist/browser/starfish.cjs2.min.js",
  "scripts": {
    "test": "mocha",
    "clean": "rm -rf ./dist/ ./doc/ ./coverage ./.nyc_output",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts src/",
    "build": "npm run clean && npm run build:tsc",
    "build:docs": "typedoc --mode modules --out ./docs/ ./src/",
    "run": "ts-node",
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "lcov",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DEX-Company/starfish-js.git"
  },
  "keywords": [],
  "author": "Dex <devops@dex.sg>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/DEX-Company/starfish-js/issues"
  },
  "homepage": "https://github.com/DEX-Company/starfish-js#README",
  "dependencies": {
    "@convex-dev/convex-api-js": "0.0.1",
    "bl": ">=2.2.1",
    "form-data": "^3.0.0",
    "js-base64": "^2.6.3",
    "mime-types": "^2.1.27",
    "node-fetch": "^2.6.0",
    "node-metamask": "^1.1.2",
    "query-string": "^6.13.0",
    "test": "^0.6.0",
    "url-join": "^4.0.1",
    "web3": "^1.2.8"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.15.18",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "browserify": "^16.5.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.2.1",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify-es": "^2.0.0",
    "gulp-util": "^3.0.8",
    "js-yaml": "^3.14.0",
    "lodash.template": ">=4.5.0",
    "mocha": "^6.2.3",
    "nyc": "^14.1.1",
    "prettier": "^2.0.5",
    "serve": "^11.3.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tsify": "^4.0.1",
    "tslint": "^5.20.1",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.7",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "yargs-parser": ">=13.1.2"
  },
  "directories": {
    "test": "test"
  },
  "publishConfig": {
    "registry":"https://npm.pkg.github.com"
  }
}
